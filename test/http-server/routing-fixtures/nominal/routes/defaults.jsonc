[
    {
        "pattern": "*",
        "inboundMiddleware": [
            [ "AuthenticationMiddleware" ],
        ],
        "outboundMiddleware": [
            [ "HttpCachingMiddleware" ],
        ],
        "errorHandlers": [
            [ "HyperviewErrorHandler" ],
        ],
        "targets": [
            {
                "methods": ["GET", "HEAD"],
                "handlers": [
                    [ "HyperviewHandler" ],
                ],
            },
        ],
    },
]
