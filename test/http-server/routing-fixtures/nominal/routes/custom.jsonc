[
    {
        "pattern": "/",
        "inboundMiddleware": [
            [ "AuthenticationMiddleware" ],
        ],
        "outboundMiddleware": [
            [ "HttpCachingMiddleware" ],
        ],
        "errorHandlers": [
            [ "HyperviewErrorHandler" ],
        ],
        "routes": [
            {
                "pattern": "/products",
                "errorHandlers": [
                    [ "ProductsErrorHandler" ],
                ],
                "routes": [
                    {
                        "pattern": "/:category_id/:product_id",
                        "targets": [
                            {
                                "methods": ["GET", "HEAD"],
                                "handlers": [
                                    [ "PublicViewProduct" ],
                                ],
                                "errorHandlers": [
                                    [ "ViewProductErrorHandler" ],
                                ],
                            },
                        ],
                    },
                ],
            },
        ],
    },
]
